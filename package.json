{
  "name": "dotup-ts-node-skills-workflows",
  "version": "0.0.5",
  "description": "alexa typescript workflow skills",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "release": "npm run clean-build && npm run test && git push && npm run gh-pages && npm publish",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf dist && tsc",
    "test": "jasmine-ts",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "postversion": "git push && git push --tags",
    "npm-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotupNET/dotup-ts-node-skills-workflows.git"
  },
  "keywords": [
    "typescript",
    "alexa",
    "skill",
    "workflows"
  ],
  "author": "Peter Ullrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotupNET/dotup-ts-node-skills-workflows/issues"
  },
  "homepage": "https://github.com/dotupNET/dotup-ts-node-skills-workflows#readme",
  "dependencies": {
    "dotup-ts-logger": "^0.0.13",
    "dotup-ts-types": "^0.0.19",
    "dotup-ts-json-serializer": "^0.0.4",
    "ask-sdk-model": "^1.12.0",
    "ask-sdk-core": "^2.4.0"
  },
  "peerDependencies": {
    "dotup-ts-node-skills": "^0.3.1"
  },
  "devDependencies": {
    "dotup-ts-node-skills": "^0.3.1",
    "@types/jasmine": "^3.3.9",
    "gh-pages": "^2.0.1",
    "jasmine": "^3.3.1",
    "jasmine-ts": "^0.3.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-ts-console-reporter": "^3.1.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3",
    "tslint": "^5.13.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-no-circular-imports": "^0.6.2",
    "grunt": "^1.0.3",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-newer": "^1.3.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "grunt-run": "^0.8.1"
  }
